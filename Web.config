  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  
  <connectionStrings>
    <add name="BAS" connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=BAS;Integrated Security=SSPI" />
    <add name="BASConnectionString" connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=BAS;Integrated Security=True;Pooling=False"
      providerName="System.Data.SqlClient" />
  </connectionStrings>


 <appSettings>     <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"></add>   </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <httpRuntime targetFramework="4.5" />
    <sessionState cookieless="UseCookies"            
                  cookieName="ASP.NET_SessionId"            
                  regenerateExpiredSessionId="true" 
                  timeout="60"    
                  mode="InProc"
    /> 
  <pages validateRequest="false" /> 
</system.web>
</configuration>